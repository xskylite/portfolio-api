[BASE URL]
WEATHER_API_URL=http://api.weatherapi.com/v1
SPOTIFY_API_URL=https://api.spotify.com/v1
SPOTIFY_ACCOUNT_URL=https://api.spotify.com/v1/me

[WEATHER]
WEATHER_API_KEY=your_weather_api_key_here

[SPOTIFY]
